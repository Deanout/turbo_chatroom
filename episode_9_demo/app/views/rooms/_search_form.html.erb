<% if current_user %>
  <%= form_with url: search_rooms_path, method: :post do |form| %>
    <%= form.search_field :name_search, value: params[:name_search], oninput: "this.form.requestSubmit()" %>
  <% end %>
  <div id="search_results">
    <%= render 'rooms/search_results', rooms: @rooms %>
  </div>
<% end %>
